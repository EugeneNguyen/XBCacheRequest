OTHER_LDFLAGS = $(inherited) -framework "AFNetworking" -framework "AVHexColor" -framework "Expecta" -framework "Expecta_Snapshots" -framework "FBSnapshotTestCase" -framework "JSONKit_NoWarning" -framework "MBProgressHUD" -framework "Specta" -framework "XBCacheRequest" -framework "XBExtension"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Tests
PODS_ROOT = ${SRCROOT}/Pods